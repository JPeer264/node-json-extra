{
  "name": "json-extra",
  "version": "1.0.1",
  "description": "json-extra gives you a little more power to json files and strings",
  "main": "dest/index.js",
  "scripts": {
    "pretest": "npm run lint",
    "test": "nyc ava",
    "lint": "eslint lib test",
    "fix": "eslint lib test --fix",
    "prepublish": "npm run build",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "build": "rimraf dest && tsc && babel dest -d dest",
    "prepush": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JPeer264/node-json-extra.git"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "author": "Jan Peer St√∂cklmair",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JPeer264/node-json-extra/issues"
  },
  "homepage": "https://github.com/JPeer264/node-json-extra#readme",
  "keywords": [
    "json",
    "extra",
    "string",
    "file",
    "read",
    "object",
    "check"
  ],
  "dependencies": {
    "fs-extra": "^4.0.2",
    "lodash.includes": "^4.3.0",
    "object.entries": "^1.0.4",
    "universalify": "^0.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.6",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "ava": "^0.22.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-register": "^6.26.0",
    "coveralls": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "husky": "^0.14.3",
    "nyc": "^11.2.1",
    "rimraf": "^3.0.2",
    "typescript": "^3.8.3"
  }
}
