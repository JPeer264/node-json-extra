{
  "name": "json-extra",
  "version": "0.5.0",
  "description": "json-extra gives you a little more power to json files and strings",
  "main": "dest/index.js",
  "scripts": {
    "pretest": "npm run lint",
    "test": "nyc ava",
    "lint": "eslint lib test",
    "fix": "eslint lib test --fix",
    "prepublish": "npm run babel",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "babel": "babel lib --out-dir=dest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JPeer264/node-json-extra.git"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "author": "Jan Peer St√∂cklmair",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JPeer264/node-json-extra/issues"
  },
  "homepage": "https://github.com/JPeer264/node-json-extra#readme",
  "keywords": [
    "json",
    "extra",
    "string",
    "file",
    "read",
    "object",
    "check"
  ],
  "dependencies": {
    "fs-extra": "^1.0.0",
    "lodash.includes": "^4.3.0",
    "object.entries": "^1.0.4",
    "universalify": "^0.1.1"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-cli": "^6.23.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.2.0",
    "babel-register": "^6.26.0",
    "coveralls": "^2.11.14",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-import": "^2.2.0",
    "nyc": "^11.2.1"
  }
